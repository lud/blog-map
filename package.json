{
    "private": true,
    "scripts": {
        "build": "rimraf dist/* && rollup -c",
        "autobuild": "rollup -c -w",
        "dev": "npm run clean && npm run copy-assets && npm run autobuild",
        "copy-assets": "cp node_modules/leaflet/dist/images public/client/ -rv",
        "clean": "rimraf public/client/*",
        "start": "serve public --listen 8000",
        "postinstall": "cp modified-rollup-plugin-svelte.cjs.js node_modules\\rollup-plugin-svelte\\dist\\rollup-plugin-svelte.cjs.js -v"
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "rollup": "^0.65.2",
        "rollup-plugin-buble": "^0.19.2",
        "rollup-plugin-commonjs": "^9.1.6",
        "rollup-plugin-json": "^3.1.0",
        "rollup-plugin-node-resolve": "^3.4.0",
        "rollup-plugin-postcss": "^1.6.2",
        "rollup-plugin-svelte": "^4.3.0",
        "rollup-plugin-uglify": "^5.0.2"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "leaflet": "^1.3.4",
        "leaflet-active-area": "^1.0.2",
        "leaflet-providers": "^1.4.0",
        "left-pad": "^1.3.0",
        "rollup-plugin-replace": "^2.0.0",
        "svelte": "^2.13.4",
        "svelte-transitions": "^1.2.0"
    }
}
